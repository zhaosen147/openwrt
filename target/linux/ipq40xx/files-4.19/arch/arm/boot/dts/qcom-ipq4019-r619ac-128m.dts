#include "qcom-ipq4019-r619ac.dtsi"

/ {
	model = "P&W R619AC (128M NAND)";
	compatible = "p2w,r619ac-128m", "p2w,r619ac";

	chosen {
		bootargs-append = " root=/dev/ubiblock0_1 rootfstype=squashfs";
	};
};

&blsp1_spi1 {
	flash@0 {
		partitions {
			partition@180000 {
				label = "unused";
				reg = <0x180000 0xe80000>;
			};
		};
	};
};

&nand {
	status = "okay";

	nand@0 {
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "rootfs";
				reg = <0x0 0x8000000>;
			};
		};
	};
};

&qpic_bam {
	status = "okay";
};
