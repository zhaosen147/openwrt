. /lib/functions/uci-defaults.sh
. /lib/functions/system.sh

board_config_update

board=$(board_name)

case "$board" in
xiaomi,ax3600)
	ucidef_set_interfaces_lan_wan "eth1 eth2 eth3" "eth0"
	;;
esac

board_config_flush

exit 0
